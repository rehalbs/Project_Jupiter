    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strict": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "types": [
            "node",
            "playwright"
        ]
    },
    "include": [
        "./**/*.ts"
    ]
}